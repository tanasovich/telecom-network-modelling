<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <AppSettings>
        <!-- N -->
        <fourierTransformBase>512</fourierTransformBase>
        <!-- n -->
        <carrierFrequencyMaxNumber>200</carrierFrequencyMaxNumber>
        <!-- L -->
        <protectionIntervalSamplesNumber>32</protectionIntervalSamplesNumber>
        <!-- m -->
        <firstChannelNumber>30</firstChannelNumber>
        <!-- R -->
        <impulseReactionLength>60</impulseReactionLength>
        <!-- from_lt -->
        <firstSample>0</firstSample>
        <!-- until_lt -->
        <lastSample>150</lastSample>
        <!-- Input files. -->
        <impulseReactionsFilename>impulse-reactions.data</impulseReactionsFilename>
        <maskFilename>mask.data</maskFilename>
        <!-- Name of file for results. Maybe, we should have several output files. -->
        <resultFilename>output.data</resultFilename>
    </AppSettings>
</configuration>
